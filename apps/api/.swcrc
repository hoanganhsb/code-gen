{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": false
      },
      "target": "es2021",
      "keepClassNames": true,
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
    },
    "module": {
      "type": "commonjs",
      "noInterop": false
    },
    "sourceMaps": true,
  }